{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "seller_name_address": {
      "type": "string"
    },
    "buyer_name_address": {
      "type": "string"
    },
    "ship_to_party": {
      "type": "string"
    },
    "container_stuffing_location": {
      "type": "string"
    },
    "consolidator_stuffer": {
      "type": "string"
    },
    "importer_of_record": {
      "type": "string"
    },
    "manufacturer_name_address": {
      "type": "string"
    },
    "consignee": {
      "type": "string"
    },
    "country_of_origin": {
      "type": "string"
    },
    "commodity_description": {
      "type": "string"
    },
    "commodity_code": {
      "type": "string"
    },
    "vessel_voyage": {
      "type": "string"
    },
